<?xml version="1.0" encoding="utf-8" ?>
<registry>
	<HKEY_ROOT>
		<Lumin>
			<REG_DWORD Name="Beta" Value="2" /> <!-- LuminOS Beta 2 -->
			<LuminSDK>
				<REG_QWORD Name="InstalledVersion" Value="0.45.04" />
				<REG_DWORD Name="TargetBuild" Value="2097" />
				<REG_DWORD Name="CanIhaveOverlay" Value="1" />
				<REG_DWORD Name="Edition" Value="20260126" />
			</LuminSDK>
			<CommunitySDK>
				<REG_SZ Name="Channel" Value="Trunk" />
				<REG_DWORD Name="InstalledVersion" Value="0" />
				<REG_DWORD Name="CanIuseCommunitySDK" Value="0"/> <!-- Community SDK doesnt have any compilations yet, so you cant use CommunitySDK by default -->
			</CommunitySDK>
			<Container>
				<REG_SZ Name="RunLocation" Value="~/container/LuminOS" />
				<REG_DWORD Name="IsInstalled" Value="1" />
			</Container>
		</Lumin>
		<PhantomUI>
			<REG_DWORD Name="IsRunning" Value="0" />
			<REG_DWORD Name="PhantomCompilation" Value="2" /> <!-- 0 = Python; 1 = C#; 2 = C++ -->
		</PhantomUI>
	</HKEY_ROOT>
</registry>
